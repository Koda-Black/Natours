{
  "name": "natours",
  "version": "1.0.0",
  "source": "./public/js/index.js",
  "description": "Changing your life with great tours",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "SET NODE_ENV=production && nodemon server.js",
    "start:bug": "nodemon --inspect server.js",
    "watch:js": "parcel watch ./public/js/index.js --dist-dir ./public/js --target browser",
    "build:js": "parcel build ./public/js/index.js --dist-dir ./public/js --target browser"
  },
  "targets": {
    "browser": {
      "outputFormat": "esmodule",
      "distDir": "./public/js",
      "source": "./public/js/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Koda-Black/natours.git"
  },
  "keywords": [
    "vacation",
    "booking",
    "app",
    "hotels",
    "node.js",
    "mongodb",
    "express",
    "postman"
  ],
  "author": "Koda-Black",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Koda-Black/natours/issues"
  },
  "homepage": "https://github.com/Koda-Black/natours#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@getbrevo/brevo": "^1.0.1",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "html-to-text": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.13.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "parcel": "^2.9.3",
    "pug": "^3.0.2",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "stripe": "^13.8.0",
    "validator": "^13.11.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3"
  },
  "engines": {
    "node": "^16"
  }
}
